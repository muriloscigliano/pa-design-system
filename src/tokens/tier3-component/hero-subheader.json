{
  "hero-subheader": {
    "text": {
      "$type": "color",
      "$value": "{color.surface.container.text}"
    },
    "size": {
      "sm": {
        "$type": "dimension",
        "$value": "{font.size.md}"
      },
      "md": {
        "$type": "dimension",
        "$value": "{font.size.lg}"
      },
      "lg": {
        "$type": "dimension",
        "$value": "{font.size.400}"
      }
    },
    "line-height": {
      "$type": "number",
      "$value": 1.5
    },
    "font": {
      "weight": {
        "$type": "string",
        "$value": "{font.weight.regular}"
      }
    }
  }
}

