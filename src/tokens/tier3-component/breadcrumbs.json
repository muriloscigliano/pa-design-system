{
  "breadcrumbs": {
    "text": {
      "default": {
        "$type": "color",
        "$value": "{color.surface.container.text}"
      },
      "link": {
        "$type": "color",
        "$value": "{color.action.primary.text.default}"
      },
      "separator": {
        "$type": "color",
        "$value": "{color.surface.container.icon}"
      }
    },
    "font": {
      "size": {
        "$type": "dimension",
        "$value": "{font.size.lg}"
      }
    },
    "spacing": {
      "item-gap": {
        "$type": "dimension",
        "$value": "{spacing.100}"
      }
    }
  }
}

