{
  "sheet": {
    "background": {
      "$type": "color",
      "$value": "{color.surface.container.background}"
    },
    "overlay": {
      "background": {
        "$type": "color",
        "$value": "rgba(0, 0, 0, 0.5)"
      },
      "z-index": {
        "$type": "dimension",
        "$value": "{z-index.400}"
      }
    },
    "shadow": {
      "$type": "shadow",
      "$value": "{shadow.lg}"
    },
    "transition": {
      "duration": {
        "default": {
          "$type": "duration",
          "$value": "{transition.duration.default}"
        }
      },
      "easing": {
        "default": {
          "$type": "string",
          "$value": "{transition.easing.default}"
        }
      }
    },
    "size": {
      "sm": {
        "width": {
          "$type": "dimension",
          "$value": "300px"
        },
        "height": {
          "$type": "dimension",
          "$value": "300px"
        }
      },
      "md": {
        "width": {
          "$type": "dimension",
          "$value": "400px"
        },
        "height": {
          "$type": "dimension",
          "$value": "400px"
        }
      },
      "lg": {
        "width": {
          "$type": "dimension",
          "$value": "600px"
        },
        "height": {
          "$type": "dimension",
          "$value": "600px"
        }
      }
    }
  }
}

