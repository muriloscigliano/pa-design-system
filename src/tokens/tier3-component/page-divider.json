{
  "page-divider": {
    "border": {
      "$type": "color",
      "$value": "{color.surface.container.border}"
    },
    "border-width": {
      "$type": "dimension",
      "$value": "{border.width.thin}"
    },
    "text": {
      "$type": "color",
      "$value": "{color.surface.base.text}"
    },
    "background": {
      "$type": "color",
      "$value": "{color.surface.base.background}"
    },
    "spacing": {
      "sm": {
        "$type": "dimension",
        "$value": "{spacing.300}"
      },
      "md": {
        "$type": "dimension",
        "$value": "{spacing.400}"
      },
      "lg": {
        "$type": "dimension",
        "$value": "{spacing.600}"
      }
    },
    "label": {
      "padding": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.200}"
        }
      },
      "font": {
        "size": {
          "$type": "dimension",
          "$value": "{font.size.lg}"
        },
        "weight": {
          "$type": "string",
          "$value": "{font.weight.semibold}"
        }
      }
    }
  }
}

