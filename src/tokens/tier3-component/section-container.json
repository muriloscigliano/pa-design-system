{
  "section-container": {
    "background": {
      "$type": "color",
      "$value": "{color.surface.container.background}"
    },
    "border": {
      "$type": "color",
      "$value": "{color.surface.container.border}"
    },
    "border-radius": {
      "$type": "dimension",
      "$value": "{border.radius.medium}"
    },
    "padding": {
      "x": {
        "$type": "dimension",
        "$value": "{spacing.300}"
      },
      "y": {
        "$type": "dimension",
        "$value": "{spacing.300}"
      }
    },
    "border-width": {
      "$type": "dimension",
      "$value": "{border.width.thin}"
    },
    "spacing": {
      "header": {
        "$type": "dimension",
        "$value": "{spacing.200}"
      },
      "content": {
        "$type": "dimension",
        "$value": "{spacing.300}"
      },
      "header-gap": {
        "$type": "dimension",
        "$value": "{spacing.200}"
      },
      "actions-gap": {
        "$type": "dimension",
        "$value": "{spacing.100}"
      },
      "title-bottom": {
        "$type": "dimension",
        "$value": "{spacing.50}"
      }
    },
    "header": {
      "border": {
        "$type": "color",
        "$value": "{color.surface.base.divider}"
      },
      "border-width": {
        "$type": "dimension",
        "$value": "{border.width.thin}"
      }
    },
    "title": {
      "font": {
        "size": {
          "$type": "dimension",
          "$value": "{font.size.400}"
        },
        "weight": {
          "$type": "string",
          "$value": "{font.weight.bold}"
        }
      },
      "color": {
        "$type": "color",
        "$value": "{color.surface.container.text}"
      }
    },
    "description": {
      "font": {
        "size": {
          "$type": "dimension",
          "$value": "{font.size.lg}"
        },
        "weight": {
          "$type": "string",
          "$value": "{font.weight.regular}"
        }
      },
      "color": {
        "$type": "color",
        "$value": "{color.surface.container.icon}"
      }
    }
  }
}

