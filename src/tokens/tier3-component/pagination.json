{
  "pagination": {
    "item": {
      "background": {
        "default": {
          "$type": "color",
          "$value": "{color.surface.base.background}"
        },
        "hover": {
          "$type": "color",
          "$value": "{color.surface.cards.hover.background}"
        },
        "active": {
          "$type": "color",
          "$value": "{color.action.primary.background.default}"
        }
      },
      "text": {
        "default": {
          "$type": "color",
          "$value": "{color.surface.base.text}"
        },
        "active": {
          "$type": "color",
          "$value": "{color.action.primary.text.default}"
        }
      },
      "border": {
        "$type": "color",
        "$value": "{color.surface.container.border}"
      },
      "padding": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.150}"
        },
        "y": {
          "$type": "dimension",
          "$value": "{spacing.100}"
        }
      },
      "radius": {
        "$type": "dimension",
        "$value": "{border.radius.medium}"
      },
      "border": {
        "width": {
          "$type": "dimension",
          "$value": "{border.width.thin}"
        }
      },
      "font": {
        "size": {
          "$type": "dimension",
          "$value": "{font.size.lg}"
        },
        "weight": {
          "$type": "string",
          "$value": "{font.weight.regular}"
        }
      },
      "size": {
        "min-width": {
          "$type": "dimension",
          "$value": "{spacing.100}"
        },
        "height": {
          "$type": "dimension",
          "$value": "{spacing.100}"
        }
      }
    },
    "gap": {
      "$type": "dimension",
      "$value": "{spacing.50}"
    }
  }
}

